package Testing;

import java.awt.AWTException;

public class Main {

	public static void main(String[] args) {
		
		TextTest robotprogramme = null;
		try {
			robotprogramme = new TextTest();
		} catch (AWTException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		robotprogramme.textTester();
	}

}
